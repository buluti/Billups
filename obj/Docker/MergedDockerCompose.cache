name: dockercompose5037800158775140585
services:
  rpsls.api:
    build:
      context: D:\projects\code\Billups\RPSLS
      dockerfile: src/RPSLS.Api/Dockerfile
    container_name: rpsls.api
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:5001;http://+:5000
    image: rpslsapi
    networks:
      default: null
    ports:
      - mode: ingress
        target: 5000
        protocol: tcp
      - mode: ingress
        target: 5000
        published: "5000"
        protocol: tcp
      - mode: ingress
        target: 5001
        protocol: tcp
      - mode: ingress
        target: 5001
        published: "5001"
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\bky\AppData\Roaming/Microsoft/UserSecrets
        target: /root/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\bky\AppData\Roaming/ASP.NET/Https
        target: /root/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: D:\projects\code\Billups\RPSLS\.containers\logs
        target: /logs
        bind:
          create_host_path: true
  rpsls.db:
    container_name: rpsls.db
    environment:
      POSTGRES_DB: rpsls-db
      POSTGRES_PASSWORD: postgres
      POSTGRES_USER: postgres
    image: postgres:latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 5432
        published: "5432"
        protocol: tcp
    volumes:
      - type: bind
        source: D:\projects\code\Billups\RPSLS\.containers\database
        target: /var/lib/postgresql/data
        bind:
          create_host_path: true
networks:
  default:
    name: dockercompose5037800158775140585_default